	<typeAlias  alias="RFCLoginVO" type="egovframework.rfc3.login.vo.LoginVO"/>
    
	<select id="RFCLoginDAO.jdcIamEnc"  parameterClass="RFCLoginVO"   resultClass="egovMap">
				  				   
				  <!--
				  select xx1.enc_varchar2_ins(#password#,10,'PASSWORD') from dual
				  -->			  
				  SELECT 'WRqNKZT4C0mmihnU0isZVcRrCB9GlV+FcIN9fPqrbgE=' as aa FROM dual
	</select>
	
	<select id="RFCLoginDAO.getJdcIam"  parameterClass="RFCLoginVO"   resultClass="egovMap">
				  
				  <!--
				  select LOGIN_PASSWD from IAMADM.JCH1001T where USER_ID = #id#
				  -->
				  SELECT 'WRqNKZT4C0mmihnU0isZVcRrCB9GlV+FcIN9fPqrbgE=77' as bb FROM dual				  
	</select>
     
