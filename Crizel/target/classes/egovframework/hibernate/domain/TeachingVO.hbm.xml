<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.teachingtool.vo">
	<class name="ToolsVO"	table="RFC_COMTNTOOLS">
	  <cache usage="read-write"/>
	  <comment>상품등록  테이블</comment>
		<id name="toolsSid"  type="long">
			<column name="TOOLS_SID">
				<comment>고유번호</comment>
            </column>
		</id>
		
		<property name="categoryId" type="string" >
			<column name="CATEGORY_ID" length="64">
				<comment>상품 분류코드</comment>
			</column>
		</property>
		<property name="toolsName" type="string" >
			<column name="TOOLS_NAME" length="100"   >
				<comment>상품 분류명</comment>
			</column>
		</property>
		<property name="gauge" type="string" >
			<column name="GAUGE" length="30"   >
				<comment>규격코드</comment>
			</column>
		</property>
		<property name="unit" type="string" >
			<column name="UNIT" length="2"   >
				<comment>단위코드</comment>
			</column>
		</property>
		<property name="totalCnt" type="long" >
			<column name="TOTAL_CNT"   >
				<comment>상품 보유수량 코드</comment>
			</column>
		</property>
		<property name="toolsContent"  type="text"   >
			<column name="TOOLS_CONTENT"  length="1073741824" >
				<comment>상품설명 </comment>
			</column>
		</property>
		<property name="fileName" type="string" >
			<column name="FILE_NAME" length="30"   >
				<comment>교구 대표이지미 논리적</comment>
			</column>
		</property>
		<property name="fileMask" type="string" >
			<column name="FILE_MASK" length="30"   >
				<comment>교구 대표이미지 물리적</comment>
			</column>
		</property>
		<property name="isUse" type="yes_no" >
			<column name="IS_USE"  length="1"  default="'1'"  >
				<comment>사용여부</comment>
			</column>
		</property>
		<property name="createDate" type="timestamp" >
			<column name="CREATE_DATE">
				<comment>등록일</comment>
			</column>
		</property>
		<property name="isDelivered" type="string" >
			<column name="IS_DELIVERED" length="5"   default="'1'"  >
				<comment>택배가능여부</comment>
			</column>
		</property>
	</class>

	<class name="ToolsOrder"	table="RFC_COMTNTOOLS_ORDER">
	  <cache usage="read-write"/>
	  <comment>상품주문  테이블</comment>
	  
		
		
		<id name="toolsOrderId"  type="string">
			<column name="TOOLS_ORDER_ID" length="20" >
				<comment>주문 고유번호</comment>
            </column>
		</id>
		
		
		<property name="aplyAreaCd"  type="string" >
			<column name="APLY_AREA_CD" length="10">
				<comment>지역청 코드</comment>
			</column>
		</property>
		<property name="aplySchoolCd"  type="string" >
			<column name="APLY_SCHOOL_CD" length="10">
				<comment>유치원 코드</comment>
			</column>
		</property>
		<property name="orderUserId"  type="string" >
			<column name="ORDER_USER_ID" length="64"   >
				<comment>주문자 아이디</comment>
			</column>
		</property>
		<property name="orderUserNm"  type="string" >
			<column name="ORDER_USER_NM" length="30"   >
				<comment>주문자 이름</comment>
			</column>
		</property>
		<property name="orderUserTel"  type="string" >
			<column name="ORDER_USER_TEL" length="20"   >
				<comment>주문자 전화번호</comment>
			</column>
		</property>
		<property name="deliverZip"  type="string" >
			<column name="DELIVER_ZIP" length="7"   >
				<comment>배송지 우편번호</comment>
			</column>
		</property>
		<property name="deliverAddr1"  type="string" >
			<column name="DELIVER_ADDR1" length="100"   >
				<comment>상세주소1</comment>
			</column>
		</property>
		<property name="deliverAddr2"  type="string" >
			<column name="DELIVER_ADDR2"  length="100"   >
				<comment>상세주소2</comment>
			</column>
		</property>
		<property name="deliverUserNm"  type="string" >
			<column name="DELIVER_USER_NM"  length="30"   >
				<comment>배송자 이름</comment>
			</column>
		</property>
		<property name="deliverUserTel"  type="string" >
			<column name="DELIVER_USER_TEL"  length="20"   >
				<comment>배송자 전화번호</comment>
			</column>
		</property>
		<property name="howToDelivered"  type="string" >
			<column name="HOWTO_DELIVERED"  length="10"   >
				<comment>택배 대여방법</comment>
			</column>
		</property>
		<property name="howToReturn"  type="string" >
			<column name="HOWTO_RETURN"  length="10"   >
				<comment>택배 반남여부</comment>
			</column>
		</property>
		<property name="orderStatus" type="string" >
			<column name="ORDER_STATUS" length="2"   >
				<comment>주문상태</comment>
			</column>
		</property>
		<property name="sdate" type="string" >
			<column name="SDATE" length="10"   >
				<comment>사용기간 시작</comment>
			</column>
		</property>
		<property name="edate" type="string" >
			<column name="EDATE" length="10"   >
				<comment>사용기간 종료</comment>
			</column>
		</property>
		<property name="regDate" type="timestamp" >
			<column name="REG_DATE">
				<comment>주문일</comment>
			</column>
		</property>
	</class>
	
	<class name="ToolsOrderGoods"	table="RFC_COMTNTOOLS_ORDERGOODS">
	  <cache usage="read-write"/>
		<comment>상품주문  테이블</comment>
	  
		<id name="toolsOrdGoodSid"  type="long">
			<column name="TOOLS_ORD_GOOD_SID">
				<comment>주문상품 교유번호</comment>
            </column>
		</id>
		
		<property name="toolsSid" type="long" >
			<column name="TOOLS_SID"  >
				<comment>주문상품 식별코드</comment>
			</column>
		</property>
		<property name="toolsOrderId" type="string" >
			<column name="TOOLS_ORDER_ID" length="20"   >
				<comment>주문번호 식별코드</comment>
			</column>
		</property>
		<property name="orderCnt" type="long" >
			<column name="ORDER_CNT"   >
				<comment>주문수량</comment>
			</column>
		</property>
		<property name="regDate" type="timestamp" >
			<column name="REG_DATE">
				<comment>등로일</comment>
			</column>
		</property>
	</class>
	
	<class name="ToolsBasket"	table="RFC_COMTNTOOLS_BASKET">
	  <cache usage="read-write"/>
		
		<comment>상품주문  테이블</comment>
	  
		<id name="basSid"  type="long">
			<column name="BAS_SID">
				<comment>장바구니 고유번호</comment>
            </column>
		</id>
		
		<property name="toolsSid" type="long" >
			<column name="TOOLS_SID"  >
				<comment>상품 고유번호</comment>
			</column>
		</property>
		<property name="sessionKey" type="string" >
			<column name="SESSION_KEY" length="20"   >
				<comment>세션 번호</comment>
			</column>
		</property>
		<property name="orderCnt" type="long" >
			<column name="ORDER_CNT"   >
				<comment>주문수량</comment>
			</column>
		</property>
		<property name="regDate" type="timestamp" >
			<column name="REG_DATE">
				<comment>등록일</comment>
			</column>
		</property>
	</class>
	
	
	<class name="PlacardStatisVO"	table="RFC_COMTNTOOLS_PLACARD_STAT">
	  <cache usage="read-write"/>
	  <comment>상품결재  테이블</comment>
		
		<id name="dataSid"  type="long">
			<column name="DATA_SID">
				<comment>결재정보  고유번호</comment>
            </column>
		</id>
		
		
	   <property name="aplyAreaCd"  type="string" >
			<column name="APLY_AREA_CD" length="10">
				<comment>지역청 코드</comment>
			</column>
		</property>
		<property name="aplySchoolCd"  type="string" >
			<column name="APLY_SCHOOL_CD" length="10">
				<comment>유치원 코드</comment>
			</column>
		</property>
		<property name="hyeonSizeCnt" type="int" >
			<column name="HYEONSIZE_CNT"   >
				<comment>결재 수량</comment>
			</column>
		</property>
		<property name="ipGanSizeCnt" type="int" >
			<column name="IPGANSIZE_CNT"   >
				<comment>결재 아이피</comment>
			</column>
		</property>
		<property name="descSizeCnt" type="int" >
			<column name="DESCSIZE_CNT"   >
				<comment>결재 위치코드</comment>
			</column>
		</property>
		<property name="etcSizeCnt" type="int" >
			<column name="ETCSIZE_CNT" >
				<comment>결재 구분</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>