<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.electuser.vo">
<class name="ElectUserVO" table="RFC_COMTNELECTUSER">
		<cache usage="read-write"/>
		<comment>교육정보 사용자 테이블</comment>
		
		<id name="userSid"  type="long">
			<column name="USER_SID">
				<comment>사용자 고유번호</comment>
            </column>
        	<generator class="native" />
		</id>
		<property name="userBirthDay" type="string" >
			<column name="USER_BIRTH_DAY" length="128"  >
				<comment>사용자 생일</comment>
			</column>
		</property>
		<property name="userRegNum" type="string" >
			<column name="USER_REGNUM" length="128" >
				<comment>사용자 인증코드</comment>
			</column>
		</property>
		<property name="userName" type="string" >
			<column name="USER_NAME" length="128"  >
				<comment>사용자 이름</comment>
			</column>
		</property>
		<property name="sex" type="string" >
			<column name="SEX" length="6" >
				<comment>성별</comment>
			</column>
		</property>
		<property name="pollingArea" type="string" >
			<column name="POLLING_AREA" length="255" >
				<comment>활동지역</comment>
			</column>
		</property>
		<property name="electAddress" type="string" >
			<column name="ELECT_ADDRESS" length="255" >
				<comment>선출 주소</comment>
			</column>
		</property>
		<property name="userAddress" type="string" >
			<column name="USER_ADDRESS" length="255" >
				<comment>사용자 주소</comment>
			</column>
		</property>
		
		<property name="userEtc1" type="string" >
			<column name="USER_ETC1" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
		<property name="userEtc2" type="string" >
			<column name="USER_ETC2" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
		<property name="userEtc3" type="string" >
			<column name="USER_ETC3" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
		<property name="userEtc4" type="string" >
			<column name="USER_ETC4" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
		<property name="userEtc5" type="string" >
			<column name="USER_ETC5" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
	</class>
	
	<class name="ElectUserLogVO"	table="RFC_COMTNELECTLOG">
		<cache usage="read-write"/>
		<comment>교육정보 사용자로그  테이블</comment>
		
		<id name="userSid"  type="long">
			<column name="USER_SID">
				<comment>사용자 고유번호</comment>
            </column>
        	<generator class="native" />
		</id>
		
		<property name="userBirthDay" type="string" >
			<column name="USER_BIRTH_DAY" length="128"  >
				<comment>사용자 생일</comment>
			</column>
		</property>
		<property name="userRegNum" type="string" >
			<column name="USER_REGNUM" length="128" >
				<comment>사용자 인증코드</comment>
			</column>
		</property>
		<property name="userName" type="string" >
			<column name="USER_NAME" length="128"  >
				<comment>사용자 이름</comment>
			</column>
		</property>
		<property name="sex" type="string" >
			<column name="SEX" length="6" >
				<comment>사용자 성별</comment>
			</column>
		</property>
		<property name="pollingArea" type="string" >
			<column name="POLLING_AREA" length="255" >
				<comment>활동 지역</comment>
			</column>
		</property>
		<property name="electAddress" type="string" >
			<column name="ELECT_ADDRESS" length="255" >
				<comment>선출 지역</comment>
			</column>
		</property>
		<property name="userAddress" type="string" >
			<column name="USER_ADDRESS" length="255" >
				<comment>사용자 주소</comment>
			</column>
		</property>
		
		<property name="userEtc1" type="string" >
			<column name="USER_ETC1" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
		<property name="userEtc2" type="string" >
			<column name="USER_ETC2" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
		<property name="userEtc3" type="string" >
			<column name="USER_ETC3" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
		<property name="userEtc4" type="string" >
			<column name="USER_ETC4" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
		<property name="userEtc5" type="string" >
			<column name="USER_ETC5" length="100" default="''" >
				<comment>확장필드</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>