			<column name="GRADE_SID">
			<comment>만족도 고유 ID</comment>
			</column>
		</id>
		<property name="userId" type="string" >
			<column name="USER_ID" length="64">
			<comment>사용자 ID</comment>
			</column>
		</property>
		<property name="userIp" type="string" >
			<column name="USER_IP" length="64">
			<comment>사용자 IP</comment>
			</column>
		</property>
		<property name="userName" type="string" >
			<column name="USER_NAME" length="64">
			<comment>사용자 이름</comment>
			</column>
		</property>
		<property name="menuCd" type="string" >
			<column name="MENU_CD" length="22" >
			<comment>메뉴 코드</comment>
			</column>
		</property>
		<property name="menuGradeCount" type="int" >
			<column name="MENU_GRADE_COUNT" length="2" default="0" >
			<comment>만족도 점수</comment>
			</column>
		</property>
		<property name="menuGradeMemo" type="string" >
			<column name="MENU_GRADE_MEMO" length="255">
			<comment>만족도 메뉴</comment>
			</column>
		</property>
		<property name="register_dt" type="timestamp" >
			<column name="REGISTER_DATE">
			<comment>등록일</comment>
			</column>
		</property>
	</class>

