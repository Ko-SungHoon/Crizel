<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.sns.vo">
	<class name="SnsInfoVO"	table="RFC_COMTNSNSINFO">
		<cache usage="read-write"/>
		<comment>SNS정보 테이블</comment>
		<id name="snsSid" type="long">
			<column name="SNS_SID">
				<comment>SNS 고유ID</comment>
			</column>
		</id>
		<property name="userId" type="string" >
			<column name="USER_ID" length="64">
				<comment>사용자ID</comment>
			</column>
		</property>
		<property name="snsType" type="string" >
			<column name="SNS_TYPE" length="30">
				<comment>SNS 종류</comment>
			</column>
		</property>
		<property name="snsIdType" type="string" >
			<column name="SNS_ID_TYPE" length="16">
				<comment>SNS ID 종류</comment>
			</column>
		</property>
		<property name="snsId" type="string" >
			<column name="SNS_ID" length="64">
				<comment>SNS 아이디</comment>
			</column>
		</property>
		<property name="snsPw" type="string" >
			<column name="SNS_PW" length="64">
				<comment>SNS 비밀번호</comment>
			</column>
		</property>
		<property name="snsNote" type="string" >
			<column name="SNS_NOTE" length="512">
				<comment>SNS 내용</comment>
			</column>
		</property>

		<property name="modify_dt" type="timestamp" >
			<column name="MODIFY_DATE" >
				<comment>수정일</comment>
			</column>
		</property>	
		<property name="register_dt" type="timestamp" >
			<column name="REGISTER_DATE" >
				<comment>등록일</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>