<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.question.vo">
	<class name="QuestionVO"	table="RFC_COMTNQUESTION">
		<cache usage="read-write"/>
		<comment>설문조사관리 테이블</comment>
		<id name="questionSid" type="long">
			<column name="QUESTION_SID">
				<comment>설문조사 고유번호</comment>
			</column>
		</id>
		<property name="sgroupId" type="string"  not-null="true" >
			<column name="SGROUP_ID" length="30"  >
			<comment>사이트그룹 아이디</comment>
			</column>
		</property>
		<property name="domainId" type="string"  not-null="true" >
			<column name="DOMAIN_ID" length="30"  >
			<comment>도메인 아이디</comment>
			</column>
		</property>
		<property name="skinId" type="string" >
			<column name="SKIN_ID" length="30"  >
			<comment>설문스킨 아이디</comment>
			</column>
		</property>
		<property name="questionType" type="string" >
			<column name="QUESTION_TYPE" length="16"  >
			<comment>설문종류</comment>
			</column>
		</property>
		<property name="questionTitle" type="string" >
			<column name="QUESTION_TITLE" length="255"  >
			<comment>설문제목</comment>
			</column>
		</property>
		<property name="questionContent" type="text" >
			<column name="QUESTION_CONTENT" length="1073741824"  >
			<comment>설문내용</comment>
			</column>
		</property>
		<property name="startDate" type="string" >
			<column name="START_DATE" length="12"  >
			<comment>설문 시작일</comment>
			</column>
		</property>
		<property name="endDate" type="string" >
			<column name="END_DATE" length="12"  >
			<comment>설문 종료일</comment>
			</column>
		</property>
	
		<property name="questionUseFl" type="string" >
			<column name="QUESTION_USE_FL" length="1"  >
			<comment>사용유무</comment>
			</column>
		</property>
		<property name="questionOpenFl" type="string" >
			<column name="QUESTION_OPEN_FL" length="1"  >
			<comment>공개여부</comment>
			</column>
		</property>
		<property name="resultOpenFl" type="string" >
			<column name="RESULT_OPEN_FL" length="1"  default="'Y'">
			<comment>결과공개여부</comment>
			</column>
		</property>
		<property name="answerDuplicateFl" type="string" >
			<column name="ANSWER_DUPLICATE_FL" length="1"  >
			<comment>중복참여가능여부</comment>
			</column>
		</property>
		<property name="answerCount" type="long" >
			<column name="ANSWER_COUNT" default="0">
			<comment>참가 인원수</comment>
			</column>
		</property>
		
		<property name="lotArea" type="string" >
			<column name="LOT_AREA" length="30"  >
			<comment>추첨 기준지역</comment>
			</column>
		</property>
		<property name="lotPercent" type="int" >
			<column name="LOT_PERCENT" default="0"  >
			<comment>기준 추첨비율</comment>
			</column>
		</property>
		<property name="lotCount" type="int" >
			<column name="LOT_COUNT" default="0"  >
			<comment>이벤트 추첨인 수</comment>
			</column>
		</property>
		
		<property name="userId" type="string" >
			<column name="USER_ID" length="64"  >
			<comment>등록한 유저 아이디</comment>
			</column>
		</property>
		<property name="register_dt" type="timestamp" >
			<column name="REGISTER_DATE" >
			<comment>등록일</comment>
			</column>
		</property>
		
		
		
		
		<property name="numQuest" type="string" >
			<column name="NUM_QUEST" length="100"   default="'0'" />
		</property>
		
	</class>
	
	<class name="AnQuestionVO"	table="RFC_COMTNANQUESTION">
		<cache usage="read-write"/>
		<comment>문항관리 테이블</comment>
		<id name="anQuestionSid" type="long">
			<column name="ANQUESTION_SID">
				<comment>문항 고유번호</comment>
			</column>
		</id>
		<property name="questionSid" type="long"  not-null="true" >
			<column name="QUESTION_SID"  >
			<comment>설문/이벤트 ID</comment>
			</column>
		</property>
		<property name="statisStandard" type="string" >
			<column name="STATIS_STANDARD" length="12"  >
			<comment>설문 통계기준</comment>
			</column>
		</property>
		<property name="questionText" type="text" >
			<column name="QUESTION_TEXT" length="1073741824"  >
			<comment>문항 내용</comment>
			</column>
		</property>
		<property name="questionHint" type="text" >
			<column name="QUESTION_HINT" length="1073741824"  >
			<comment>문항 정답 힌트</comment>
			</column>
		</property>		
		<property name="itemType" type="int" >
			<column name="ITEM_TYPE"  >
			<comment>문항 응답 형식</comment>
			</column>
		</property>		
		<property name="orderNum" type="int" >
			<column name="ORDER_NUM" default="0" >
			<comment>문항 순서</comment>
			</column>
		</property>
		<property name="headContent" type="string" >
			<column name="HEAD_CONTENT" length="200"  >
			<comment>문항 머릿말</comment>
			</column>
		</property>
		
		<property name="groupIdx" type="int" >
			<column name="GROUP_IDX" default="0" >
			<comment>문항 그룹</comment>
			</column>
		</property>
		<property name="groupOrder" type="int" >
			<column name="GROUP_ORDER" default="0" >
			<comment>문항 그룹 순서</comment>
			</column>
		</property>
		
		<property name="questionNum" type="string" >
			<column name="QUESTION_NUM" length="64"  >
				<comment>문항 출력 번호</comment>
			</column>
		</property>
		
		
		<property name="fileNm" type="string" >
			<column name="FILE_NAME" length="255"/>
		</property>
		<property name="fileMask" type="string" >
			<column name="FILE_MASK" length="255"/>
		</property>
		
		
	</class>
	
	<class name="AnQuestionItemVO"	table="RFC_COMTNANQUESTIONITEM">
		<cache usage="read-write"/>
		<comment>예문관리 테이블</comment>
		<id name="itemQuestionSid" type="long">
			<column name="ITEMQUESTION_SID">
				<comment>예문 고유번호</comment>
			</column>
		</id>
		<property name="questionSid" type="long"  not-null="true" >
			<column name="QUESTION_SID"  >
			<comment>설문/이벤트 고유ID</comment>
			</column>
		</property>
		<property name="anQuestionSid" type="long"  not-null="true" >
			<column name="ANQUESTION_SID"  >
			<comment>문항 고유ID</comment>
			</column>
		</property>		
		<property name="itemText" type="text" >
			<column name="ITEM_TEXT" length="1073741824"  >
			<comment>문항내용</comment>
			</column>
		</property>
		<property name="itemAnswer" type="string" >
			<column name="ITEM_ANSWER" length="3"  >
			<comment>문항 정답여부</comment>
			</column>
		</property>		
		<property name="itemIdx" type="int" >
			<column name="ITEM_IDX"  >
			<comment>문항 출력 순서</comment>
			</column>
		</property>	
		<property name="linkSid" type="long"  not-null="true" >
			<column name="LINK_SID"  default="0">
			<comment>문항 연결 설문 ID</comment>
			</column>
		</property>	
	</class>
	
	<class name="QuestionResultVO"	table="RFC_COMTNQUESTIONRESULT">
		<cache usage="read-write"/>
		<comment>설문결과 테이블</comment>
		<id name="resultSid" type="long">
			<column name="RESULT_SID">
				<comment>설문결과 고유번호</comment>
			</column>
		</id>
		<property name="userInfoSid" type="long"  not-null="true" >
			<column name="USERINFO_SID"  default="0">
			<comment>이벤트 참가자 고유ID</comment>
			</column>
		</property>
		<property name="questionSid" type="long"  not-null="true" >
			<column name="QUESTION_SID"  >
			<comment>설문/이벤트 고유ID</comment>
			</column>
		</property>
		<property name="anQuestionSid" type="long"  not-null="true" >
			<column name="ANQUESTION_SID"  >
			<comment>문항 고유ID</comment>
			</column>
		</property>		
		<property name="itemQuestionSid" type="long"  not-null="true" >
			<column name="ITEMQUESTION_SID"  >
			<comment>선택한 예문 고유ID</comment>
			</column>
		</property>
		<property name="subjectAnswer" type="text" >
			<column name="SUBJECT_ANSWER" length="1073741824"  >
			<comment>주관식 답</comment>
			</column>
		</property>
		<property name="register_dt" type="timestamp" >
			<column name="REGISTER_DATE" >
			<comment>등록일</comment>
			</column>
		</property>	
	</class>
	<class name="QuestionUserInfoVO"	table="RFC_COMTNQUESTIONUSERINFO">
		<cache usage="read-write"/>
		<comment>설문참여자정보 테이블</comment>
		<id name="userInfoSid" type="long">
			<column name="USERINFO_SID">
				<comment>참여자 고유번호</comment>
			</column>
		</id>
		<property name="questionSid" type="long"  not-null="true" >
			<column name="QUESTION_SID"  >
			<comment>설문/이벤트 고유ID</comment>
			</column>
		</property>
		
		<property name="userName" type="string" >
			<column name="USER_NAME" length="128"  >
			<comment>참여자 이름</comment>
			</column>
		</property>
		<property name="userRegNo" type="string" >
			<column name="USER_REG_NO" length="256" >
			<comment>참여자 가상식별번호</comment>
			</column>
		</property>
		<property name="userZipCode" type="string" >
			<column name="USER_ZIPCODE" length="16" >
			<comment>참여자 우편번호</comment>
			</column>
		</property>
		<property name="userAddress" type="string" >
			<column name="USER_ADDRESS" length="512" >
			<comment>참여자 주소</comment>
			</column>
		</property>
		<property name="userCel" type="string" >
			<column name="USER_CEL" length="32" >
			<comment>참여자 연락처</comment>
			</column>
		</property>
		<property name="userEmail" type="string" >
			<column name="USER_EMAIL" length="256" >
			<comment>참여자 이메일</comment>
			</column>
		</property>
		<property name="questionYear" type="string" >
			<column name="QUESTION_YEAR" length="8" >
			<comment>참여 년도</comment>
			</column>
		</property>
		<property name="mailingLink" type="string" >
			<column name="MAILING_LINK" length="2" >
			<comment>메일링 여부</comment>
			</column>
		</property>
		<property name="questionDate" type="string" >
			<column name="QUESTION_DATE" length="16" >
			<comment>참가일자</comment>
			</column>
		</property>
		
		<property name="register_dt" type="timestamp" >
			<column name="REGISTER_DATE" >
			<comment>등록일자</comment>
			</column>
		</property>	
	</class>
</hibernate-mapping>