<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.user.vo">
       <class name="ZipCodeVO" table="RFC_COMTZIPCODE_TB">
			<cache usage="read-only" />
			<comment>구 주소정보 테이블</comment>
			<id name="seq" type="int">
				<column name="SEQ">
					<comment>우편번호 고유ID</comment>
				</column>
			</id>
			<property name="zipcode" type="string" >
				<column name="ZIPCODE" length="7" >
					<comment>우편번호</comment>
				</column>
			</property>
			<property name="sido" type="string" >
				<column name="SIDO" length="4" >
					<comment>시 도</comment>
				</column>
			</property>
			<property name="gugun" type="string" >
				<column name="GUGUN" length="15" >
					<comment>구 군</comment>
				</column>
			</property>
			<property name="dong" type="string" >
				<column name="DONG" length="52" >
					<comment>읍 동 면</comment>
				</column>
			</property>
			<property name="bunji" type="string" >
				<column name="BUNJI" length="17" >
					<comment>번지</comment>
				</column>
			</property>
		</class>
		
		<class name="NewZipCodeVO" table="RFC_COMTNEW_ZIPCODE_TB">
			<cache usage="read-only" />
			<comment>신 주소정보 테이블</comment>
			<id name="bldgMno" type="string">
				<column name="BLDG_MNO" length="25">
					<comment>건물관리번호</comment>
				</column>
			</id>
			<property name="codeDong" type="string" >
				<column name="CODE_DONG" length="10">
					<comment>법정도코드</comment>
				</column>
			</property>
			<property name="sido" type="string" >
				<column name="SIDO" length="40" >
					<comment>시도명</comment>
				</column>
			</property>
			<property name="gugun" type="string" >
				<column name="GUGUN" length="40" >
					<comment>시군구명</comment>
				</column>
			</property>
			<property name="dong" type="string" >
				<column name="DONG" length="40" >
					<comment>법정읍면동명</comment>
				</column>
			</property>
			<property name="ri" type="string" >
				<column name="RI" length="40">
					<comment>법정리명</comment>
				</column>
			</property>
			<property name="chkMountain" type="string" >
				<column name="CHK_MOUNTAIN" length="1">
					<comment>산여부</comment>
				</column>
			</property>
			<property name="lotNum" type="string" >
				<column name="LOT_NUM" length="4">
					<comment>지번본번</comment>
				</column>
			</property>
			<property name="lotSubNum" type="string" >
				<column name="LOT_SUB_NUM" length="4">
					<comment>지번부번</comment>
				</column>
			</property>
			<property name="roadCode" type="string" >
				<column name="ROAD_CODE" length="12" >
					<comment>도로명 코드</comment>
				</column>
			</property>
			<property name="roadName" type="string" >
				<column name="ROAD_NAME" length="12" >
					<comment>도로명 이름</comment>
				</column>
			</property>
			<property name="chkLowerGround" type="string" >
				<column name="CHK_LOWER_GROUND" length="1">
					<comment>지하여부</comment>
				</column>
			</property>
			<property name="bldgNum" type="string" >
				<column name="BLDG_NUM" length="5" >
					<comment>건물본번</comment>
				</column>
			</property>
			<property name="bldgSubNum" type="string" >
				<column name="BLDG_SUB_NUM" length="5">
					<comment>건물부번</comment>
				</column>
			</property>
			<property name="bldgName" type="string" >
				<column name="BLDG_NAME" length="40" >
					<comment>건물명</comment>
				</column>
			</property>
			<property name="bldgDetailName" type="string" >
				<column name="BLDG_DETAIL_NAME" length="100">
					<comment>상세 건물명</comment>
				</column>
			</property>
			<property name="dongNo" type="string" >
				<column name="DONGNO" length="2" >
					<comment>읍면동 일련번호</comment>
				</column>
			</property>
			<property name="dongCode" type="string" >
				<column name="DONG_CODE" length="10" >
					<comment>행정동 코드</comment>
				</column>
			</property>
			<property name="dongName" type="string" >
				<column name="DONG_NAME" length="20" >
					<comment>행정동명</comment>
				</column>
			</property>
			<property name="zipCode" type="string" >
				<column name="ZIP_CODE" length="6" >
					<comment>우편일련번호</comment>
				</column>
			</property>
			<property name="zipCodeNo" type="string" >
				<column name="ZIP_CODE_NO" length="3" >
					<comment>우편번호</comment>
				</column>
			</property>
		</class>
</hibernate-mapping>