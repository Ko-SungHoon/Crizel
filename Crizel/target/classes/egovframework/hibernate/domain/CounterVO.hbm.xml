<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.counter.vo">
	<class name="CounterVO"  table="RFC_COMTNCOUNTER">
		<cache usage="read-write"/>
		<comment>접속 통계 정보 테이블</comment>
		<id name="counterSid" type="long">
			<column name="COUNTER_SID">
			<comment>접속 통계 고유 ID</comment>
			</column>
		</id>
		<property name="qrcodeSid"  type="long" >
			<column name="QRCODE_SID" default="0"  >
			<comment>QR코드 ID</comment>
			</column>
		</property>
		<property name="sgroupId" type="string" >
			<column name="SGROUP_ID" length="30" >
			<comment>사이트 그룹 ID</comment>
			</column>
		</property>
		<property name="domainId" type="string" >
			<column name="DOMAIN_ID" length="30" >
			<comment>도메인 ID</comment>
			</column>
		</property>
		<property name="accessDate" type="string" >
			<column name="ACCESS_DATE" length="8">
			<comment>접속일자</comment>
			</column>
		</property>
		<property name="accessTime" type="string" >
			<column name="ACCESS_TIME" length="6">
			<comment>접속시간</comment>
			</column>
		</property>
		<property name="dayOfWeek" type="int" >
			<column name="DAY_OF_WEEK" >
			<comment>접속 요일</comment>
			</column>
		</property>
		<property name="qrType" type="string" >
			<column name="QRTYPE" length="30"  >
			<comment>접속 형식</comment>
			</column>
		</property>
		<property name="userId" type="string" >
			<column name="USER_ID" length="30"  >
			<comment>접속자 ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
