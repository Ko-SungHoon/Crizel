<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.fooderp.vo">
	<class name="PackMainVO"	table="T_PACK_MAIN">
		<cache usage="read-write"/>
		<id name="packNo" column="packNo"  type="int" />		
		<property name="packDate" type="string" >
			<column name="packDate" length="10"/>
		</property>
		<property name="packTypeCd" type="string" >
			<column name="packTypeCd" length="2"/>
		</property>
		<property name="register" type="string" >
			<column name="register" length="20"/>
		</property>
		<property name="weekOrder" type="int" >
			<column name="weekOrder" />
		</property>
		<property name="year" type="string" >
			<column name="year" length="4"/>
		</property>
		<property name="regiDate" type="timestamp" >
			<column name="regiDate"/>
		</property>
	</class>
	
	<class name="CodeMstSubVO"	table="T_CODE_MST_SUB">
		<cache usage="read-write"/>
		<composite-id>
			<key-property name="mainCd" column="MAINCD"  type="string" length="2"/>
			<key-property name="subCd" column="SUBCD" type="string"  length="2" />					
		</composite-id>
				
		<property name="subNm" type="string" >
			<column name="SUBNM" length="100"/>
		</property>
		<property name="subCdValue" type="string" >
			<column name="SUBCDVALUE" length="100"/>
		</property>	
	</class>
	
	<class name="PlanSubVO"	table="T_DELIVERY_PLAN_SUB">
		<cache usage="read-write"/>
		<composite-id>
			<key-property name="deliSid" column="DELISID"  type="int"/>
			<key-property name="packNo" column="PACKNO" type="int" />					
			<key-property name="prodCd" column="PRODCD" type="string" length="8" />
		</composite-id>
						
		<property name="packunitamt" type="int" >
			<column name="PACKUNITAMT" />
		</property>
		<property name="packunitcd" type="string" >
			<column name="PACKUNITCD" length="2"/>
		</property>	
		<property name="unitprice" type="int" >
			<column name="UNITPRICE"/>
		</property>
	</class>
	
	<class name="DeliveryPlanVO"	table="T_DELIVERY_PLAN">
		<cache usage="read-write"/>
		<id name="deliSid" column="deliSid"  type="int" />		
		<property name="deadLineYN" type="string" >
			<column name="deadLineYN" length="1" default="'N'"/>
		</property>
		<property name="deliConNm" type="string" >
			<column name="deliConNm" length="50"/>
		</property>
		<property name="deliIndex" type="string" >
			<column name="deliIndex" length="2"/>
		</property>
		<property name="deliKindCd" type="string" >
			<column name="deliKindCd" length="2"/>
		</property>
		<property name="deliPlanDate" type="string" >
			<column name="deliPlanDate" length="10"/>
		</property>
		<property name="deliPlanWeekDay" type="string" >
			<column name="deliPlanWeekDay" length="2"/>
		</property>
		<property name="deliTotIndex" type="string" >
			<column name="deliTotIndex" length="2"/>
		</property>
		<property name="editDate" type="timestamp" >
			<column name="editDate"/>
		</property>
		<property name="editor" type="string" >
			<column name="editor" length="20"/>
		</property>
		<property name="invoiceNo" type="string" >
			<column name="invoiceNo" length="32"/>
		</property>
		<property name="memId" type="string" >
			<column name="memId" length="20"/>
		</property>
		<property name="memoYN" type="string" >
			<column name="memoYN" length="1" default="'N'"/>
		</property>
		<property name="multiOrderNo" type="string" >
			<column name="multiOrderNo" length="32"/>
		</property>
		<property name="orderDate" type="string" >
			<column name="orderDate" length="10"/>
		</property>
		<property name="orderNo" type="string" >
			<column name="orderNo" length="32"/>
		</property>
		<property name="packTypeCd" type="string" >
			<column name="packTypeCd" length="2"/>
		</property>
		<property name="prodOrderYN" type="string" >
			<column name="prodOrderYN" length="1" default="'N'"/>
		</property>
		<property name="receiveAddr" type="string" >
			<column name="receiveAddr" length="150"/>
		</property>
		<property name="receiveDetailAddr" type="string" >
			<column name="receiveDetailAddr" length="150"/>
		</property>		
		<property name="receiveMobileNo" type="string" >
			<column name="receiveMobileNo" length="20"/>
		</property>
		<property name="receiveNm" type="string" >
			<column name="receiveNm" length="30"/>
		</property>
		<property name="receivePostNo" type="string" >
			<column name="receivePostNo" length="7"/>
		</property>
		<property name="receiveTelNo" type="string" >
			<column name="receiveTelNo" length="20"/>
		</property>
		<property name="regiDate" type="timestamp" >
			<column name="regiDate"/>
		</property>
		<property name="register" type="string" >
			<column name="register" length="20"/>
		</property>
		<property name="shipmentNo" type="string" >
			<column name="shipmentNo" length="10"/>
		</property>
		<property name="statusCd" type="string" >
			<column name="statusCd" length="2"/>
		</property>
	</class>
</hibernate-mapping>