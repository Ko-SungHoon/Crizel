<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.iam.vo">
	<class name="SiteGroupVO"	table="RFC_COMTNSITEGROUP">
	  <cache usage="read-write"/>
	    <comment>사이트 그룹 테이블</comment>
		<id name="sgroupId"  type="string" >
            <column name="SGROUP_ID">
				 <comment>사이트그룹 고유ID</comment>
            </column>
        </id> 
		<property name="sgroupNm" type="string" >
			<column name="SGROUP_NM" length="50">
				 <comment>사이트그룹명</comment>
			</column>
		</property>
		<property name="isUse" type="yes_no" >
			<column name="IS_USE"  length="1"  default="'1'">
				 <comment>사용여부</comment>
			</column>
		</property>
		<property name="createDate"  type="timestamp"  insert="true" update="false">
			<column name="CREATE_DATE"   >
				 <comment>등록일</comment>
			</column>
		</property>
		<property name="modifyDate"  type="timestamp"  insert="false" update="true">
			<column name="MODIFY_DATE"  >
				<comment>수정일</comment>
			</column>
		</property>		
	</class>
</hibernate-mapping>