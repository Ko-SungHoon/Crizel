<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.user.vo">
	<class name="MberManageVO"	table="RFC_COMTNMEMBER">
	  <cache usage="read-write"/>
	  <comment>일반사용자 테이블</comment>
		<id name="uniqId" type="string"  length="20" >
			<column name="UNIQ_ID">
				<comment>일반 사용자 일련번호</comment>
			</column>
		</id>    
		<property name="mberId" type="string" >
			<column name="MBER_ID" length="64" unique="true"  not-null="true"  >
				<comment>일반 사용자 ID</comment>
			</column>
		</property>
		<property name="mberNm" type="string" >
			<column name="MBER_NM" length="60"  >
				<comment>이름</comment>
			</column>
		</property>
		<property name="ihidnum" type="string" >
			<column name="IHID_NUM" length="64"  >
				<comment>가상식별번호</comment>
			</column>
		</property>
		<property name="password" type="string" >
			<column name="PASSWORD" length="128"  >
				<comment>패스워드</comment>
			</column>
		</property>
		<property name="passwordCnsr" type="string" >
			<column name="PASSWORD_CNSR" length="100"  >
				<comment>패스워드 찾기 답변</comment>
			</column>
		</property>
		<property name="passwordHint" type="string" >
			<column name="PASSWORD_HINT" length="100"  >
				<comment>패스워드 찾기 힌트</comment>
