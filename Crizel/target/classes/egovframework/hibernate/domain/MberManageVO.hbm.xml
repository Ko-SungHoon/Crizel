			<column name="SUB_DN"  length="100"  >
				<comment>인증서 인증 값</comment>
			</column>
		</property>
		<property name="photoImage"  type="string" >
			<column name="PHOTO_IMAGE"  length="20"  >
				<comment>프로필 이미지 경로</comment>
			</column>
		</property>
		<property name="userLevel"  type="int" >
			<column name="USER_LEVEL" default="0" >
