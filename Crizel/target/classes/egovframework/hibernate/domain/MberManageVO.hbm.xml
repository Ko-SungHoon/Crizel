		</property>
		<property name="hakYear"  type="string" >
			<column name="HAK_YEAR"  length="4"  >
				<comment>입학년도</comment>
			</column>
		</property>
		<property name="hakYeon"  type="string" >
			<column name="HAK_YEON"  length="4"  >
				<comment>학년</comment>
			</column>
		</property>
		<property name="banNo"  type="string" >
			<column name="BAN_NO"  length="4"  >
