			</column>
		</property>
		<property name="adres" type="string" >
			<column name="ADRES" length="255"  >
				<comment>주소</comment>
			</column>
		</property>
		<property name="detailAdres" type="string" >
			<column name="DETAIL_ADRES" length="255"  >
