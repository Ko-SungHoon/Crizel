			</column>
		</property>
		<property name="sbscrbDe"  type="timestamp"  insert="true" update="false">
			<column name="SBSCRB_DE"    >
				<comment>가입 일자</comment>
			</column>
		</property>
		<property name="modifyDate"  type="timestamp"  insert="false" update="true">
			<column name="MODIFY_DATE"    >
				<comment>정보수정 일자</comment>
			</column>
		</property>
		<property name="parentName" type="string" >
			<column name="PARENT_NAME" length="60"  >
				<comment>부모님 이름</comment>
			</column>
		</property>
		<property name="parentTel" type="string" >
