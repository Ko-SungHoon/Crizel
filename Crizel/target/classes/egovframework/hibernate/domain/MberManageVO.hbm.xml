			</column>
		</property>
		<property name="moblfristNo" type="string" >
			<column name="MOBLFRIST_NO" length="128"  >
				<comment>휴대전화번호1</comment>
			</column>
		</property>
		<property name="moblmiddleNo" type="string" >
			<column name="MOBLMIDDLE_NO" length="128"  >
