				<comment>추가필드1</comment>
			</column>
		</property>
		<property name="userEtc6" type="string" >
			<column name="USER_ETC6" length="100"  >
				<comment>추가필드1</comment>
			</column>
		</property>
		<property name="userEtc7" type="string" >
			<column name="USER_ETC7" length="100"  >
				<comment>추가필드1</comment>
			</column>
		</property>
		<property name="userEtc8" type="string" >
			<column name="USER_ETC8" length="100"  >
				<comment>추가필드1</comment>
			</column>
		</property>
		<property name="userEtc9" type="string" >
			<column name="USER_ETC9" length="100"  >
				<comment>추가필드1</comment>
			</column>
		</property>
		<property name="userEtc10" type="string" >
			<column name="USER_ETC10" length="100"  >
				<comment>추가필드1</comment>
			</column>
		</property>
				
		<property name="userAuthCode" type="string">
			<column name="USER_AUTH_CODE" length="128" >
				<comment>암호화 방법</comment>
			</column>
		</property>
		
		<property name="pwChangeDate"  type="timestamp"  insert="false" update="false">
			<column name="PWCHANGE_DATE"  >
				<comment>최종 패스워드 변경일</comment>
			</column>
		</property>
		
		<property name="loginFirstEventYn" type="string" >
			<column name="LOGIN_FIRST_EVENT_YN"  length="1"   default="'1'" >
				<comment>최초 로그인 이벤트 유무</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>