<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.iam.vo">
	<class name="AdminMenuRolesVO"	table="RFC_COMTNADMIN_ROLES">
	  <cache usage="read-write"/>
	  <comment>관리자메뉴 권한 테이블</comment>
		<composite-id>
			<key-property name="admSid" type="string"  length="30" >
				<column name="ADM_SID">
					<comment>메뉴코드</comment>
				</column>
			</key-property>    
			<key-property name="roleId" type="string"  length="30" >
				<column name="ROLE_ID">
					<comment>권한 이름</comment>
				</column>
			</key-property>    
		</composite-id>
	</class>
	<class name="GroupMenuRolesVO"	table="RFC_COMTNGROUP_ROLES">
	  <cache usage="read-write"/>
	  <comment>관리자메뉴 권한그룹 테이블</comment>
		<composite-id>
			<key-property name="admSid" type="string"  length="30" >
				<column name="ADM_SID">
					<comment>메뉴코드</comment>
				</column>
			</key-property>    
			<key-property name="groupId" type="string"  length="30" >
				<column name="GROUP_ID">
					<comment>그룹코드</comment>
				</column>
			</key-property>    
		</composite-id>
	</class>
</hibernate-mapping>