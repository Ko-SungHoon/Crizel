<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.iam.vo">
	<class name="GroupVO"	table="RFC_COMTNGROUP">
	  <cache usage="read-write"/>
	  <comment>회원그룹 테이블</comment>
		<id name="groupId" type="string">
			<column name="GROUP_ID" length="30">
				<comment>그룹 고유ID</comment>
            </column>
		</id>
		<property name="sgroupId" type="string"  not-null="true" >
			<column name="SGROUP_ID" length="30">
				<comment>사이트 그룹 고유ID</comment>
			</column>
		</property>
		<property name="groupNm" type="string" >
			<column name="GROUP_NM" length="50">
				<comment>회원그룹 이름</comment>
			</column>
		</property>
		<property name="groupDc" type="string" >
			<column name="GROUP_DC" length="100">
				<comment>회원그룹 설명</comment>
			</column>
		</property>
		<property name="createDate"  type="timestamp"  insert="true" update="false">
			<column name="CREATE_DATE">
				<comment>등록일자</comment>
			</column>
		</property>
		<property name="modifyDate"  type="timestamp"  insert="false" update="true">
			<column name="MODIFY_DATE">
				<comment>수정일자</comment>
			</column>
		</property>		
	</class>
</hibernate-mapping>