<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.office.vo">
	<class name="OfficeVO" table="RFC_COMTCOFFICE">
		<cache usage="read-write"/>
		<comment>조직도 테이블</comment>		
		<id name="officeCd" type="string" >
			<column name="OFFICE_CD" length="13"   >
			<comment>조직도 고유ID</comment>
			</column>		
		</id>
		<property name="sgroupId" type="string"  not-null="true" >
			<column name="SGROUP_ID" length="30"   >
			<comment>사이트그룹ID</comment>
			</column>
		</property>
		<property name="officeNm" type="string" >
			<column name="OFFICE_NM" length="30" >
			<comment>조직이름</comment>
			</column>
		</property>
		<property name="officeDp" type="int" >
			<column name="OFFICE_DP"  >
			<comment>조직차수</comment>
			</column>
		</property>
		<property name="officeSortIx" type="int" >
			<column name="OFFICE_SORT_IX"  >
			<comment>조직도 정렬순서</comment>
			</column>
		</property>
		<property name="officeParentCd" type="string" >
			<column name="OFFICE_PARENT_CD" length="13" >
			<comment>부모조직도 고유ID</comment>
			</column>
		</property>
		<property name="officeImg" type="string" >
			<column name="OFFICE_IMG" length="255"  >
			<comment>조직도이미지</comment>
			</column>
		</property>
		<property name="officeMemo" type="text" >
			<column name="OFFICE_MEMO"  length="1073741824"  >
			<comment>조직도 설명</comment>
			</column>
		</property>
		<property name="officeTel" type="string" >
			<column name="OFFICE_TEL" length="20"  >
			<comment>조직 대표 전화</comment>
			</column>
		</property>
		<property name="officeFax" type="string" >
			<column name="OFFICE_FAX" length="20" >
			<comment>조직 대표 팩스</comment>
			</column>
		</property>
		<property name="officeIsUse" type="yes_no" >
			<column name="OFFICE_IS_USE" length="1"  default="1"  >
			<comment>조직도 사용여부</comment>
			</column>
		</property>
		<property name="officeIsBoard" type="yes_no" >
			<column name="OFFICE_IS_BOARD" length="1" default="1" >
			<comment>응용쪽 출력여부</comment>
			</column>
		</property>
		<property name="officeRealCode" type="string" >
			<column name="OFFICE_REAL_CODE" length="50" >
			<comment>새올행정 코드</comment>
			</column>
		</property>
		<property name="homeOfficeIsUse" type="yes_no" >
			<column name="HOMEOFFICE_IS_USE" length="1"  default="1"  >
			<comment>조직홈 사용 여부</comment>
			</column>
		</property>
		<property name="modify_dt" type="timestamp" >
			<column name="MODIFY_DATE" >
			<comment>조직도 수정일</comment>
			</column>
		</property>
		<property name="register_dt" type="timestamp" >
			<column name="REGISTER_DATE">
			<comment>조직도 등록일</comment>
			</column>
		</property>
		
		<property name="officeTmp" type="string" >
			<column name="OFFICE_TMP" length="20"  >
			<comment>조직도 임시 필드</comment>
			</column>
		</property>
		
		<property name="portal_dt" type="timestamp" >
			<column name="PORTAL_DATE" >
			<comment>통합포털 수정일</comment>
			</column>
		</property>
	</class>
	
	<class name="OfficePartVO" table="RFC_COMTCOFFICE_PART">
		<cache usage="read-write"/>
		<comment>담당관리 테이블</comment>
		<id name="officePtSid"  type="long">
			<column name="OFFICE_PT_SID">
			<comment>담당 고유 ID</comment>
			</column>
		</id>
		<property name="sgroupId" type="string"  not-null="true" >
			<column name="SGROUP_ID" length="30"   >
			<comment>사이트 그룹 ID</comment>
			</column>
		</property>
		<property name="officeCd" type="string"  not-null="true" >
			<column name="OFFICE_CD" length="13" >
			<comment>조직도 고유ID</comment>
			</column>
		</property>
		<property name="userId" type="string" >
			<column name="USER_ID" length="64" >
			<comment>업무회원 ID</comment>
			</column>
		</property>
		<property name="userNm" type="string" >
			<column name="USER_NM" length="64" >
			<comment>업무회원 이름</comment>
			</column>
		</property>
		<property name="officeTel" type="string" >
			<column name="OFFICE_TEL" length="20"  >
			<comment>행정전화번호</comment>
			</column>
		</property>
		<property name="officeFax" type="string" >
			<column name="OFFICE_FAX" length="20" >
			<comment>행정팩스번호</comment>
			</column>
		</property>
		<property name="officePtIx" type="int" >
			<column name="OFFICE_PT_IX"  >
			<comment>서열</comment>
			</column>
		</property>
		<property name="officePtIsUse" type="yes_no" >
			<column name="OFFICE_PT_IS_USE" length="1" default="1" >
			<comment>사용여부</comment>
			</column>
		</property>
		<property name="officePtMemo" type="text" >
			<column name="OFFICE_PT_MEMO"  length="1073741824"  >
			<comment>업무소개</comment>
			</column>
		</property>
		<property name="modify_dt" type="timestamp" >
			<column name="MODIFY_DATE" >
			<comment>업무수정일</comment>
			</column>
		</property>
		<property name="register_dt" type="timestamp" >
			<column name="REGISTER_DATE">
			<comment>업무등록일</comment>
			</column>
		</property>

	</class>
	
	
	
</hibernate-mapping>