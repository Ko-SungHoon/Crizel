<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.sns.vo">
	<class name="SnsCommentVO"	table="RFC_SNS_COMMENT">
	  <cache usage="read-write"/>
	  <comment>SNS 코멘트 테이블</comment>
		<id name="snsCmtSid" type="long">
			<column name="SNS_CMT_SID">
				<comment>SNS 고유ID</comment>
			</column>
		</id>
		<property name="categoryId" type="string"  not-null="true" >
			<column name="CATEGORY_ID" length="64">
				<comment>프로그램ID</comment>
			</column>
		</property>
		
		<property name="dataIdx" type="int" >
			<column name="DATA_IDX" default="0">
				<comment>그룹번호</comment>
			</column>
		</property>
		<property name="dataRef" type="int" >
			<column name="DATA_REF" default="0">
				<comment>원글및 답변여부</comment>
			</column>
		</property>
		<property name="dataDep" type="int" >
			<column name="DATA_DEP" default="1">
				<comment>글 차수</comment>
			</column>
		</property>
		
		<property name="snsUserId" type="string" >
			<column name="SNS_USER_ID" length="64">
				<comment>SNS사용자 ID</comment>
			</column>
		</property>
		<property name="snsUserNickNm" type="string" >
			<column name="SNS_USER_NICK_NM" length="64">
				<comment>SNS사용자 닉네임</comment>
			</column>
		</property>
		
		<property name="snsUserIp" type="string" >
			<column name="SNS_USER_IP" length="16">
				<comment>접속 아이피</comment>
			</column>
		</property>
		
		<property name="snsType" type="string" >
			<column name="SNS_TYPE" length="30">
				<comment>SNS종류</comment>
			</column>
		</property>
		
		<property name="snsProfileImgUrl" type="string" >
			<column name="SNS_PROFILE_IMG_URL" length="200">
				<comment>SNS사용자 프로파일 이미지</comment>
			</column>
		</property>
		
		<property name="snsProfileUrl" type="string" >
			<column name="SNS_PROFILE_URL" length="200">
				<comment>SNS사용자 프로파일 주소</comment>
			</column>
		</property>
		
		<property name="snsContent" type="string" >
			<column name="SNS_CONTENT" length="140">
				<comment>SNS 내용</comment>
			</column>
		</property>

		
		<property name="imgFileName" type="string" >
			<column name="IMG_FILE_NAME" length="255">
				<comment>SNS 업로드 이미지명</comment>
			</column>
		</property>
		<property name="imgFileMask" type="string" >
			<column name="IMG_FILE_MASK" length="255">
				<comment>SNS 업로드 이미지 MASK</comment>
			</column>
		</property>
		<property name="imgFileSize" type="string" >
			<column name="IMG_FILE_SIZE" length="64">
				<comment>SNS 업로드 이미지 사이즈</comment>
			</column>
		</property>
		
		<property name="snsRegDate" type="timestamp" >
			<column name="SNS_REG_DATE">
				<comment>SNS 등록일</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>