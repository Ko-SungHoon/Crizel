<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.iam.vo">
	<class name="ViewResourceVO"	table="RFC_COMTNVIEWRESOURCE">
	  <cache usage="read-write"/>
	  	<comment>VIEW자원 권한매핑 테이블</comment>
		
		<id name="viewResourceId"  type="string">
			<column name="VIEW_RESOURCE_ID" length="30" >
				<comment>고유번호</comment>
            </column>
		</id>
		
		<property name="sgroupId" type="string" >
			<column name="SGROUP_ID" length="30"   >
				<comment>사이트 식별코드</comment>
			</column>
		</property>
		<property name="category" type="string" >
			<column name="CATEGORY" length="30"    unique="true"  not-null="true" >
				<comment>자원 카테고리</comment>
			</column>
		</property>
		<property name="viewName" type="string" >
			<column name="VIEW_NAME" length="50"   >
				<comment>VIEW 자원이름</comment>
			</column>
		</property>
		<property name="description" type="string" >
			<column name="DESCRIPTION" length="255"   >
				<comment>VIEW 상세정보</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>