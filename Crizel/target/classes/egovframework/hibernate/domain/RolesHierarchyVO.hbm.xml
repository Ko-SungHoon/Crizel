<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.iam.vo">
	<class name="RolesHierarchyVO"	table="RFC_COMTNROLES_HIERARCHY">
	  <cache usage="read-write"/>
		<comment>권한 계층 테이블</comment>
		<composite-id>
			<key-property name="parentRole" type="string">
				<column name="PARENT_ROLE" length="30">
					 <comment>부모권한ID</comment>
				</column>
			</key-property>
			<key-property name="childRole"  type="string"  length="30">
				<column name="CHILD_ROLE">
            		<comment>자식권한ID</comment>
            	</column>
			</key-property>					
		</composite-id>
		<property name="createDate"  type="timestamp"  insert="true" update="false">
			<column name="CREATE_DATE">
				<comment>등록일</comment>
			</column>
		</property>
		<property name="modifyDate"  type="timestamp"  insert="false" update="true">
			<column name="MODIFY_DATE">
				<comment>수정일</comment>
			</column>
		</property>		
	</class>
</hibernate-mapping>