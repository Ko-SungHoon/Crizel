<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.openapi20.vo">
	<class name="OpenApiQosVO"	table="RFC_COMTOPENAPIQOS">
	  <cache usage="read-write"/>
		<comment>OpenAPI 품질 관리 테이블</comment>
		<id name="qosSid" type="long">
			<column name="QOS_SID">
			<comment>OpenAPI 품질 고유ID</comment>
			</column>
		</id>
		<property name="serviceUrl" type="string">
			<column name="SERVICE_URL" length="300">
			<comment>OpenAP 서비스 URL</comment>
			</column>
		</property>
		<property name="queryString" type="string">
			<column name="QUERY_STRING" length="2000">
			<comment>OpenAPI 서비스 쿼리</comment>
			</column>
		</property>
		<property name="openApiAuthKey" type="string">
			<column name="OPEN_API_AUTHKEY" length="512" not-null="true">
			<comment>OpenAPI 인증키</comment>
			</column>
		</property>
		<property name="startTime" type="long">
			<column name="START_TIME" >
			<comment>OpenAPI 서비스 시작시간</comment>
			</column>
		</property>
		<property name="endTime" type="long" >
			<column name="END_TIME" >
			<comment>OpenAPI 서비스 종료시간</comment>
			</column>
		</property>
		<property name="differenceTime" type="long" >
			<column name="DIFFERENCE_TIME" >
			<comment>OpenAPI 호출 사용시간</comment>
			</column>
		</property>
		<property name="status" type="string" >
			<column name="STATUS" length="10"  >
			<comment>OpenAPI 서비스 호출 상태</comment>
			</column>
		</property>
		<property name="failReason" type="string" >
			<column name="FAIL_REASON" length="2000"  >
			<comment>OpenAPI 서비스 호출 실패 사유</comment>
			</column>
		</property>
		<property name="regDate" type="timestamp" >
			<column name="REG_DATE">
			<comment>등록일</comment>
			</column>
		</property>
		<property name="userIp" type="string" >
			<column name="USER_IP"  length="25">
			<comment>OpenAPI 서비스 호출자 IP</comment>
			</column>
		</property>
		
	</class>
	
	<class name="OpenApiStatusVO"	table="RFC_COMTOPENAPI_STATUS">
	  <cache usage="read-write"/>
      <comment>OpenApi 호출 목록 테이블</comment>
	  <id name="stSid" type="long">
			<column name="ST_SID">
			<comment>OpenApi 호출 목록 고유 ID</comment>
			</column>
	  </id>
	    <property name="stName" type="string">
			<column name="ST_NAME" length="100">
			<comment>OpenApi 호출 이름</comment>
			</column>
		</property>
		<property name="stUrl" type="string">
			<column name="ST_URL" length="300">
			<comment>OpenApi 호출 url</comment>
			</column>
		</property>
		<property name="stParam" type="string" >
			<column name="ST_PARAM" length="300">
			<comment>OpenApi 호출 파라미터</comment>
			</column>
		</property>
		<property name="stVal" type="long" >
			<column name="ST_VAL" default="0">
			<comment>OpenApi 호출 파라미터값</comment>
			</column>
		</property>
		<property name="gubun" type="string" >
			<column name="GUBUN" length="10">
			<comment>OpenApi 호출 형식</comment>
			</column>
		</property>
		<property name="stIdx" type="int" >
			<column name="ST_IDX" default="0">
			<comment>OpenApi 호출 순서</comment>
			</column>
		</property>
		
	</class>
	
	<class name="OpenApiStatiscVO"	table="RFC_COMTOPENAPI_STATICS">
	  <cache usage="read-write"/>
	  <comment>OpenApi 요청 관리 테이블</comment>
	  <id name="apiRfcStaticSid" type="long">
			<column name="API_RFC_STATIC_SID">
			<comment>OpenApi 요청 고유 ID</comment>
			</column>
	  </id>
		<property name="apiOfferSid" type="long"  >
			<column name="API_OFFER_SID">
			<comment>OpenApi 고유 ID</comment>
			</column>
		</property>
		<property name="apiUrl" type="string">
			<column name="API_URL" length="300">
			<comment>OpenApi URL</comment>
			</column>
		</property>
		<property name="usedServiceKey" type="string" >
			<column name="USED_SERVICE_KEY" length="20">
			<comment>OpenApi 개인 인증키</comment>
			</column>
		</property>
		<property name="staticDate" type="timestamp" >
			<column name="STATIC_DATE">
			<comment>OpenApi 요청 일자</comment>
			</column>
		</property>
		
		<property name="callCnt" type="long">
			<column name="CALL_CNT">
			<comment>OpenApi 호출 수</comment>
			</column>
		</property>
		
		<property name="errCnt" type="long">
			<column name="ERR_CNT">
			<comment>OpenApi 호출 에러 수</comment>
			</column>
		</property>
		
		<property name="maxTime" type="long">
			<column name="MAX_TIME">
			<comment>OpenApi 요청 최대 시간</comment>
			</column>
		</property>
		
		<property name="avgTime" type="float">
			<column name="AVG_TIME">
			<comment>OpenApi 요청 평균 시간</comment>
			</column>
		</property>
	</class>
	
	
	
</hibernate-mapping>