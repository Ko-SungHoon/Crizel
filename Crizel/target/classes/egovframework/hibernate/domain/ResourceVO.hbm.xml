<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="egovframework.rfc3.iam.vo">
	<class name="ResourceVO"	table="RFC_COMTNRESOURCE">
	  <cache usage="read-write"/>
		<comment>자원 함수 테이블</comment>
		<id name="resourceId"  type="string" >
            <column name="RESOURCE_ID">
				<comment>자원 고유ID</comment>
            </column>
        </id> 
		<property name="sgroupId" type="string"   not-null="true">
			<column name="SGROUP_ID" length="30"   >
				<comment>사이트그룹ID</comment>
			</column>
		</property>
		<property name="domainId" type="string" >
			<column name="DOMAIN_ID" length="30"    >
				<comment>도메인고유ID</comment>
			</column>
		</property>
		<property name="resourceName" type="string" >
			<column name="RESOURCE_NAME" length="50"  >
				<comment>자원이름</comment>
			</column>
		</property>
		<property name="resourcePattern" type="string"  >
			<column name="RESOURCE_PATTERN" length="300">
				<comment>자원패턴</comment>
			</column>
		</property>
		<property name="description" type="string" >
			<column name="DESCRIPTION" length="100"  >
				<comment>자원설명</comment>
			</column>
		</property>
		<property name="resourceType" type="string" >
			<column name="RESOURCE_TYPE" length="50"   >
				<comment>자원타입</comment>
			</column>
		</property>
		<property name="sortOrder" type="long" >
			<column name="SORT_ORDER"   >
				<comment>자원정렬순서</comment>
			</column>
		</property>
		<property name="isPermit"  type="yes_no">
			<column name="IS_PERMIT"  length="1"  default="'1'"   >
				<comment>적용범위</comment>
			</column>
		</property>
		<property name="createDate"  type="timestamp"  insert="true" update="false">
			<column name="CREATE_DATE"   >
				<comment>등록일</comment>
			</column>
		</property>
		<property name="modifyDate"  type="timestamp"  insert="false" update="true">
			<column name="MODIFY_DATE"    >
				<comment>수정일</comment>
			</column>
		</property>
		<property name="menuCd" type="string" >
			<column name="MENU_CD" length="30"    >
				<comment>메뉴코드</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>