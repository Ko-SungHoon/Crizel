<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
    <formset>
		<form name="domainVO">
			<field property="sgroupId" depends="required">
        		<arg0 key="사이트그룹코드" resource="false"/>
			</field>
			<field property="domain" depends="required">
        		<arg0 key="도메인" resource="false"/>
			</field>
			<field property="domainNm" depends="required">
        		<arg0 key="도메인 이름" resource="false"/>
			</field>
			<field property="port" depends="required,integer">
        		<arg0 key="PORT" resource="false"/>
			</field>
			<field property="isSsoLogin" depends="required">
        		<arg0 key="SSO 로그인 허용여부" resource="false"/>
			</field>
			<field property="isSearch" depends="required">
        		<arg0 key="검색 인덱싱 허용여부" resource="false"/>
			</field>
			<field property="endTagUse" depends="required">
        		<arg0 key="Img  end tag 사용여부" resource="false"/>
			</field>
			<field property="isUse" depends="required">
        		<arg0 key="사용여부" resource="false"/>
			</field>
		</form>
    </formset>
</form-validation>