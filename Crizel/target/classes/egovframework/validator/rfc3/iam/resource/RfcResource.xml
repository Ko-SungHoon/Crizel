<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>
        <form name="resourceVO"> 
        	
        	<field property="isPermit" depends="required">
        		<arg0 key="도메인 접근여부" resource="false"/>
			</field>
            <field property="resourceName" depends="required">
                <arg0 key="자원이름" resource="false"/>
            </field>
             <field property="resourcePattern" depends="required">
                <arg0 key="자원패턴" resource="false"/>
            </field>
             <field property="description" depends="required">
                <arg0 key="자원설명" resource="false"/>
            </field>
            <field property="resourceType" depends="required">
                <arg0 key="자원타입" resource="false"/>
            </field>
            <field property="sortOrder" depends="required,integer">
                <arg0 key="자원접근 우선순위" resource="false"/>
            </field>
        </form>
        
        <form name="viewResourceVO"> 
        	 <field property="sgroupId" depends="required">
                <arg0 key="사이트 그룹코드" resource="false"/>
            </field>
        	<field property="category" depends="required">
        		<arg0 key="view자원코드" resource="false"/>
			</field>
            <field property="viewName" depends="required">
                <arg0 key="view자원이름" resource="false"/>
            </field>
             <field property="resourcePattern" depends="required">
                <arg0 key="자원패턴" resource="false"/>
            </field>
        </form>
       
    </formset>

</form-validation>