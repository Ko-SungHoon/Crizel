<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
    <formset>
    	<form name="openApiAuthKeyVO">
			<field property="userId" depends="required">
        		<arg0 key="사용자 아이디" resource="false"/>
			</field>
			<field property="userNm" depends="required">
        		<arg0 key="사용자 이름" resource="false"/>
			</field>
			<!-- 
			<field property="birthDay" depends="required">
        		<arg0 key="생년월일" resource="false"/>
			</field>
			 -->
		    <field property="tel" depends="required">
        		<arg0 key="연락처" resource="false"/>
			</field>
			<field property="email" depends="required">
        		<arg0 key="이메일" resource="false"/>
			</field>
			<field property="serviceNm" depends="required">
        		<arg0 key="공공정보명" resource="false"/>
			</field>
			<field property="useAim" depends="required">
        		<arg0 key="이용목적" resource="false"/>
			</field>
			<field property="startDay" depends="required">
        		<arg0 key="이용시작일" resource="false"/>
			</field>
			<field property="endDay" depends="required">
        		<arg0 key="이용종료일" resource="false"/>
			</field>
			
		</form>
    </formset>
</form-validation>